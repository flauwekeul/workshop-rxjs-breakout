{
  "name": "workshop-rxjs-breakout",
  "description": "Workshop for creating a Breakout clone in the browser with RxJS",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc -p tsconfig.build.json && vite build --base /workshop-rxjs-breakout/ && npm run build:slides",
    "build:slides": "slidev build --base /workshop-rxjs-breakout/slides/ --out dist/slides",
    "preview": "vite preview",
    "slides": "slidev",
    "exercise:1": "ts-node --esm exercises/test/01.ts",
    "exercise:2": "ts-node --esm exercises/test/02.ts",
    "exercise:3": "ts-node --esm exercises/test/03.ts",
    "exercise:4": "ts-node --esm exercises/test/04.ts"
  },
  "author": "Abbe Keultjes <flauwekeul@gmail.com> (https://abbekeultjes.nl/)",
  "license": "MIT",
  "devDependencies": {
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "vite": "4.2.1"
  },
  "dependencies": {
    "@slidev/cli": "0.40.5",
    "@slidev/theme-default": "0.21.2",
    "rxjs": "7.8.0"
  }
}
