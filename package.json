{
  "name": "workshop-rxjs-breakout",
  "version": "0.0.1",
  "description": "Workshop for creating a Breakout clone in the browser with RxJS",
  "main": "index.js",
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build && slidev build --base /workshop-rxjs-breakout/slides/ --out build/slides",
    "slides": "slidev",
    "exercise:1": "ts-node exercises/test/01.ts",
    "exercise:2": "ts-node exercises/test/02.ts",
    "exercise:3": "ts-node exercises/test/03.ts",
    "exercise:4": "ts-node exercises/test/04.ts"
  },
  "author": "Abbe Keultjes <flauwekeul@gmail.com> (https://abbekeultjes.nl/)",
  "license": "MIT",
  "devDependencies": {
    "snowpack": "3.8.8",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@slidev/cli": "0.33.1",
    "@slidev/theme-default": "0.21.2",
    "rxjs": "7.5.5"
  }
}
